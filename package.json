{
  "name": "alka-monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": "git@github.com:alkafinance/ledger-sync.git",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "pnpm run --filter next dev",
    "lint": "eslint --ext .js,.ts,.tsx --cache .",
    "mitm": "mitmweb --listen-port 3010 --web-port 3011 --mode reverse:http://localhost:3000",
    "preinstall": "npx only-allow pnpm",
    "test::ci": "pnpm run test --ci --forceExit --detectOpenHandles --runInBand",
    "test::debug": "node --inspect-brk $(pnpm bin)/jest --runInBand --watch",
    "test::watch": "pnpm run test --watch",
    "test": "jest",
    "prepare": "husky install",
    "typecheck": "tsc --project . --noEmit",
    "validate": "run-s --silent typecheck lint test",
    "ledgerSync": "tsx ./bin/ledgerSync"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,css,yml,yaml}": "prettier --write",
    "**/*.{js,ts,tsx}": "eslint --ext .js,.ts,.tsx --cache --fix"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "3.4.2",
    "@ledger-sync/cli": "*",
    "@ledger-sync/util": "*",
    "@roarr/cli": "5.7.0",
    "@types/jest": "29.0.0",
    "@types/node": "16.11.12",
    "@types/prettier": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "esbuild": "0.15.6",
    "esbuild-jest": "0.5.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-tailwindcss": "3.6.1",
    "husky": "8.0.1",
    "jest": "29.0.1",
    "jest-date-mock": "1.0.8",
    "jest-watch-typeahead": "2.1.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "tsx": "3.9.0",
    "typescript": "4.8.2"
  },
  "engines": {
    "node": "16",
    "pnpm": "7"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "core-js": "3.6.5",
      "resolve-url": "0.2.1",
      "sane": "4.1.0",
      "shvl": "2.0.3",
      "source-map-resolve": "0.5.3",
      "source-map-url": "0.4.1",
      "urix": "0.1.0"
    },
    "neverBuiltDependencies": [
      "libpq"
    ],
    "overrides": {
      "axios": "0.21.1",
      "slonik": "30.3.1"
    },
    "patchedDependencies": {
      "@trpc/server@9.26.0": "patches/@trpc__server@9.26.0.patch",
      "cac@6.7.12": "patches/cac@6.7.12.patch",
      "chrono-node@2.3.8": "patches/chrono-node@2.3.8.patch",
      "esbuild-jest@0.5.0": "patches/esbuild-jest@0.5.0.patch",
      "firebase@9.8.1": "patches/firebase@9.8.1.patch",
      "js-combinatorics@1.5.7": "patches/js-combinatorics@1.5.7.patch",
      "micro-memoize@4.0.10": "patches/micro-memoize@4.0.10.patch",
      "plaid@10.7.0": "patches/plaid@10.7.0.patch",
      "remeda@0.0.32": "patches/remeda@0.0.32.patch",
      "timm@1.7.1": "patches/timm@1.7.1.patch",
      "typescript@4.8.2": "patches/typescript@4.8.2.patch",
      "zod@3.17.3": "patches/zod@3.17.3.patch"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "slonik": "30"
      }
    }
  }
}
